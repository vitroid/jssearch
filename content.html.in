function compare(a, b) {
  // Use toUpperCase() to ignore character casing
  const bandA = a.lab;
  const bandB = b.lab;

  let comparison = 0;
  if (bandA > bandB) {
    comparison = 1;
  } else if (bandA < bandB) {
    comparison = -1;
  }
  return comparison;
}



